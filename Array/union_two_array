#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n, m, cnt = 0;
    cin >> n >> m;
    int arr1[n], arr2[m];
    unordered_map<int, int> map;
    for (int i = 0; i < n; i++)
    {
        cin>>arr1[i];
        map[arr1[i]]++;
    }
    for (int j = 0; j < m; j++)
    {
        cin>>arr2[j];
        map[arr2[j]]++;
    }
    cout<<map.size();   
}
